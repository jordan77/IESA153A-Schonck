<?xml version='1.0' encoding='utf-8'?>
<widget id="com.cordova.schonck" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="UniversalAnalytics">
        <param name="ios-package" value="UniversalAnalyticsPlugin" />
    </feature>
    <feature name="Camera">
        <param name="ios-package" value="CDVCamera" />
    </feature>
    <preference name="CameraUsesGeolocation" value="false" />
    <feature name="Contacts">
        <param name="ios-package" value="CDVContacts" />
    </feature>
    <feature name="Geolocation">
        <param name="ios-package" value="CDVLocation" />
    </feature>
    <feature name="Globalization">
        <param name="ios-package" value="CDVGlobalization" />
    </feature>
    <feature name="SplashScreen">
        <param name="ios-package" value="CDVSplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <icon height="180" src="Resources/icons/icon-60@3x.png" width="180" />
    <icon height="60" src="Resources/icons/icon-60.png" width="60" />
    <icon height="120" src="Resources/icons/icon-60@2x.png" width="120" />
    <icon height="76" src="Resources/icons/icon-76.png" width="76" />
    <icon height="152" src="Resources/icons/icon-76@2x.png" width="152" />
    <icon height="40" src="Resources/icons/icon-40.png" width="40" />
    <icon height="80" src="Resources/icons/icon-40@2x.png" width="80" />
    <icon height="57" src="Resources/icons/icon.png" width="57" />
    <icon height="114" src="Resources/icons/icon@2x.png" width="114" />
    <icon height="72" src="Resources/icons/icon-72.png" width="72" />
    <icon height="144" src="Resources/icons/icon-72@2x.png" width="144" />
    <icon height="29" src="Resources/icons/icon-small.png" width="29" />
    <icon height="58" src="Resources/icons/icon-small@2x.png" width="58" />
    <icon height="50" src="Resources/icons/icon-50.png" width="50" />
    <icon height="100" src="Resources/icons/icon-50@2x.png" width="100" />
    <name>schonck</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
